{
    "buildConfiguration": {
        "buildTasks": {
            "maven clean package": {
                "type": "maven",
                "selected": true,
                "configuration": {
                    "phase": "package",
                    "pomPath": "pom.xml",
                    "saveToBuildDir": true,
                    "enableSonarScan": true,
                    "clean": true,
                    "managedSettingsId": "maven-settings-oci",
                    "properties": [
                        "skipTests=true"
                    ],
                    "withDocker": {
                        "image": "fra.ocir.io/sisalspa/base-images/maven:3.8.3-openjdk-17"
                    }
                }
            }
        }
    },
    "containerConfiguration": {
        "branchConfiguration": {
            "develop": {
                "tagPrefix": "svil",
                "regexFilter": "develop|feature.+"
            }
        },
        "containerTasks": {
            "build docker image": {
                "type": "docker-image",
                "selected": true,
                "activeBranches": [
                    "develop"
                ],
                "configuration": {
                    "buildContext": "BUILD/maven-clean-package",
                    "dockerfile": "BUILD/maven-clean-package/dockerfile/Dockerfile",
                    "imageName": "kairos/monitor-service",
                    "buildArgs": [
                        "TARGET_PATH=BUILD/maven-clean-package/monitor-service/target"
                    ],
                    "generatedFinalImageName": "fra.ocir.io/sisalspa/kairos/monitor-service",
                    "tags": {
                        "develop": {
                            "tag": "edd3a7ce2d3a-11-svil"
                        }
                    }
                }
            }
        }
    },
    "kubernetesConfiguration": {
        "manifestsGitSCM": {
            "url": "http://git-ita.nplts.sisal.it/kairos/monitor-service-manifests.git",
            "branch": "main"
        },
        "clusterDefinitions": {
            "svil-cluster": {
                "deploymentPath": "kubernetes/svil",
                "namespaces": [
                    "kairos-common"
                ],
                "branchRegexFilter": "develop|feature.+",
                "deployAgentName": "jenkins-repnorm-ita-sisal-dev-a"
            }
        },
        "kubernetesTasks": {
            "deploy": {
                "type": "k8s_deploy",
                "activeClusters": [
                    "svil-cluster"
                ],
                "configuration": {
                    "imageFrom": [
                        "build docker image"
                    ]
                }
            }
        }
    }
}